{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "ES2020",
        "sourceMap": true,
        "moduleResolution": "node",
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "importHelpers": true,
        "experimentalDecorators": true,
        "declaration": false,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "./dist",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true
    }
}